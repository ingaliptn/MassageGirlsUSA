            <!-- service -->

            <h3>Book With Us Now</h3>
            <!-- Form -->
            <form asp-controller="Mail" asp-action="ProcessForm" enctype="multipart/form-data" method="post" class="frm-show-form  frm_pro_form " id="form_contact-form">
                <div class="frm_form_fields ">
                    <fieldset>
                        <legend class="frm_screen_reader">Contact Us</legend>

                        <div class="frm_fields_container">
                            <input type="hidden" name="" value="create">
                            <input type="hidden" name="" value="9">
                            <input type="hidden" name="" id="frm_hide_fields_9" value="">
                            <input type="hidden" name="" value="contact-form">
                            <input type="hidden" name="" value="">
                            <input type="hidden" id="" name="frm_submit_entry_9" value="59b651a00f">
                            <input type="hidden" name="" value="/girls/ivy/">

                            <label for="" class="frm_screen_reader frm_hidden">If you are human, leave this field blank.</label>
                            <input type="text" class="frm_hidden frm_verify" id="frm_verify_9" name="frm_verify" value="">


                            <div id="frm_field_94_container" class="frm_form_field form-field  frm_required_field frm_top_container frm_first frm_third">
                                <label for="" class="frm_primary_label">
                                    Name
                                    <span class="frm_required"></span>
                                </label>
                                <input type="text" id="Name" name="Name" value="" placeholder="Name" data-reqmsg="This field cannot be blank." data-invmsg="Name is invalid" required="required">
                            </div>


                            <div id="frm_field_96_container" class="frm_form_field form-field  frm_required_field frm_top_container frm_third">
                                <label for="" class="frm_primary_label">
                                    Email
                                    <span class="frm_required"></span>
                                </label>
                                <input type="email" id="Email" name="Email" value="" placeholder="Email" data-reqmsg="This field cannot be blank." data-invmsg="Please enter a valid email address" required="required">
                            </div>

                            <div id="frm_field_99_container" class="frm_form_field form-field  frm_top_container frm_third">
                                <label for="" class="frm_primary_label">
                                    Phone
                                    <span class="frm_required"></span>
                                </label>
                                <input type="text" id="Phone" name="Phone" value="" placeholder="Phone" data-frmval="Phone" data-invmsg="Text is invalid">
                            </div>

                            <div id="frm_field_99_container" class="frm_form_field form-field  frm_top_container frm_first_half">
                                <label for="" class="frm_primary_label">
                                    Preffered Model
                                    <span class="frm_required"></span>
                                </label>
                                <select name="FirstGirl" id="FirstGirl"
                                <option value="">-- Select Girl --</option>
                                    @foreach (var girlName in ViewData["Girls"] as List<string>)
                                    {
                                           <option value="@girlName">@girlName</option>
                                    }
                                    </select>
                            </div>

                            <div id="frm_field_99_container" class="frm_form_field form-field  frm_top_container frm_last_half">
                                <label for="" class="frm_primary_label">
                                    Alternative Model
                                    <span class="frm_required"></span>
                                </label>
                                <select name="SecondGirl" id="SecondGirl"
                                    <option value="">-- Select Girl --</option>
                                    @foreach (var girlName in ViewData["Girls"] as List<string>)
                                    {
                                         <option value="@girlName">@girlName</option>
                                    }
                                    </select>
                            </div>



                            <div id="frm_field_99_container" class="frm_form_field form-field  frm_top_container frm_full">
                                <label for="City" class="frm_primary_label">
