@model MassageGirls.Models.BuyNowModel

<section id="primary" class="full-width ws-narrow">
    <main id="content" role="main">
        <div class="ws-page">

            <header class="header" itemprop="name headline">
                <h1 class="page-title">Schedule An Appointment</h1>
            </header>

            <div class="content">

                <div class="ws-profile-contact" id="frm_form_9_container">

                    <form asp-controller="Mail" asp-action="ProcessForm" enctype="multipart/form-data" method="post" class="frm-show-form  frm_pro_form " id="form_contact-form">
                        <div class="frm_form_fields ">
                            <fieldset>
                                <legend class="frm_screen_reader">Contact Us</legend>

                                <div class="frm_fields_container">
                                    <input type="hidden" name="" value="create">
                                    <input type="hidden" name="" value="9">
                                    <input type="hidden" name="" id="frm_hide_fields_9" value="">
                                    <input type="hidden" name="" value="contact-form">
                                    <input type="hidden" name="" value="">
                                    <input type="hidden" id="" name="frm_submit_entry_9" value="59b651a00f">
                                    <input type="hidden" name="" value="/girls/ivy/">

                                    <label for="" class="frm_screen_reader frm_hidden">If you are human, leave this field blank.</label>
                                    <input type="text" class="frm_hidden frm_verify" id="frm_verify_9" name="frm_verify" value="">


                                    <div id="frm_field_94_container" class="frm_form_field form-field  frm_required_field frm_top_container frm_first frm_third">
                                        <label asp-for="Name" class="frm_primary_label">Name <span class="frm_required"></span></label>
                                        <input asp-for="Name" class="form-control" placeholder="Name" required />
                                        <span asp-validation-for="Name" class="text-danger"></span>
                                    </div>


                                    <div id="frm_field_96_container" class="frm_form_field form-field  frm_required_field frm_top_container frm_third">
                                        <label asp-for="Email" class="frm_primary_label">Email <span class="frm_required"></span></label>
                                        <input asp-for="Email" type="email" class="form-control" placeholder="Email" required />
                                        <span asp-validation-for="Email" class="text-danger"></span>
                                    </div>

                                    <div id="frm_field_99_container" class="frm_form_field form-field  frm_top_container frm_third">
                                        <label asp-for="Phone" class="frm_primary_label">Phone <span class="frm_required"></span></label>
                                        <input asp-for="Phone" class="form-control" placeholder="Phone" />
                                        <span asp-validation-for="Phone" class="text-danger"></span>
                                    </div>

                                    <div id="frm_field_99_container" class="frm_form_field form-field frm_top_container frm_first_half">
                                        <label asp-for="FirstGirl" class="frm_primary_label">
                                            Preferred Model
                                            <span class="frm_required"></span>
                                        </label>
                                        <select asp-for="FirstGirl" name="FirstGirl" id="FirstGirl">
                                            <option value="">-- Select First Girl --</option>
                                            @foreach (var GirtName in ViewData["Girls"] as List<string>)
                                            {
                                                <option value="@GirtName">@GirtName</option>
                                            }
                                        </select>
                                    </div>

                                    <div id="frm_field_99_container" class="frm_form_field form-field frm_top_container frm_last_half">
                                        <label asp-for="SecondGirl" class="frm_primary_label">
                                            Alternative Model
                                            <span class="frm_required"></span>
                                        </label>
                                        <select asp-for="SecondGirl" name="SecondGirl" id="SecondGirl">
                                            <option value="">-- Select Second Girl --</option>
                                            @foreach (var GirtNameS in ViewData["Girls"] as List<string>)
                                            {
                                                <option value="@GirtNameS">@GirtNameS</option>
                                            }
                                        </select>
                                    </div>

                                    <div id="frm_field_99_container" class="frm_form_field form-field  frm_top_container frm_full">
                                        <label asp-for="City" class="frm_primary_label">City <span class="frm_required"></span></label>
                                        <select asp-for="City" class="form-control" required>
                                            <option value="">-- Select City --</option>
                                            @foreach (var cityName in ViewData["TownName"] as List<string>)
                                            {
                                                <option value="@cityName">@cityName</option>
                                            }
                                        </select>
                                    </div>


                                    <div id="frm_field_98_container" class="frm_form_field form-field  frm_required_field frm_top_container frm_full">
                                        <label asp-for="Message" class="frm_primary_label">
                                            Message
                                            <span class="frm_required"></span>
                                        </label>
                                        <textarea asp-for="Message" name="Message" id="Message" rows="5" placeholder="Message" data-reqmsg="This field cannot be blank." style="resize: none;" data-invmsg="Message is invalid"></textarea>
                                    </div>

                                    <input type="hidden" name="item_key" value="">
                                    <div class="frm_submit">
                                        <button class="frm_button_submit frm_final_submit" type="submit">Submit</button>
                                    </div>

                                </div>
                            </fieldset>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </main>
</section>

