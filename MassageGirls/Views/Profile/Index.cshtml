@{
    Layout = "~/Views/Shared/_LayoutProfile.cshtml";
}
@model MassageGirls.Models.GirlProfile

<section id="primary" class="full-width ws-narrow">
    <main id="content" role="main">
        <div class="ws-page ws-profile">

            <article id="post-675" class="post-675 girl type-girl status-publish has-post-thumbnail hentry">

                <div class="divleft">
                    <div class="ibox">
                        <img width="400" height="600" src="~/images/masseuse/@Model.Photo" class="attachment-wsomega-profile size-wsomega-profile wp-post-image" alt="">
                    </div>
                </div>


                <div class="divright">
                    <h1>@Model.GirlName</h1>
                    <div class="summary">
                        @Html.Raw(Model.Bio)
                    </div>
                    <hr class="ws-clear">
