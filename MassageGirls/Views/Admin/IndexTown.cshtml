@model IEnumerable<MassageGirls.Models.Town>
@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<h2>Town Index</h2>

<table class="table" style="position:center">
    <thead>
        <tr>
            <th>Town Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var town in Model)
        {
            <tr>
                <td>@town.TownName</td>
                <td>
                    <a asp-controller="Admin" asp-action="EditTown" asp-route-id="@town.TownID" class="btn btn-primary">Update</a>
                </td>
            </tr>
        }
    </tbody>
</table>
