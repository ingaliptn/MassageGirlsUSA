@using MassageGirls.Models
<html lang="en" class="">
<head>
    <base href="/">
    <meta charset="utf-8">
    <title>Massage Girls Online</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="shortcut icon" href="~/images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/images/favicon.ico" type="image/x-icon">
    <meta name="robots" content="index, follow">
    <link rel="canonical" href="https://massagegirls.online/atlanta/" />
    <link rel="stylesheet" type="text/css" media="all" href="~/css/style.css?version=1.3">
    <link rel="dns-prefetch" href="https://use.fontawesome.com/">
    <link rel="stylesheet" id="formidable-css" href="~/css/formidableforms.css" type="text/css" media="all">
    <script src="~/js/webfont.js" type="text/javascript" async></script>
    <script type="text/javascript" src="~/js/jquery.js"></script>
    <script type="text/javascript" src="~/js/jquery-migrate.min.js"></script>

    <script type="text/javascript" src="~/js/wsthemescripts.packed.js"></script>
    <script type="text/javascript" src="~/js/wsthemescripts.js"></script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="sitemap" href="./sitemap.xml" type="application/xml" />
    <style>
        .parallax {
            /* The image used */
            background-image: url('images/las-vegas-asian-hotel-room-massage.jpg');
            /* Set a specific height */
            /*height: 500px;*/
            /* Create the parallax scrolling effect */
            background-attachment: fixed;
            background-position: center;
            background-repeat: no-repeat;
            background-size: cover;
        }
    </style>
</head>
<body class="home page-template page-template-page-section page-template-page-section-php page page-id-7">

    <!--<div class="parallax-mirror" style="visibility: visible; z-index: 0; position: fixed; top: 90px; left: 0px; overflow: hidden; transform: translate3d(0px, 0px, 0px); height: 607px; width: 1903px;">
      <img class="parallax-slider" src="images/las-vegas-asian-hotel-room-massage.jpg" style="transform: translate3d(0px, 0px, 0px); position: absolute; top: -89px; left: 0px; height: 713px; width: 1903px; max-width: none;">
    </div>-->

    <div class="ws-loader" style="display: none;">
        <div class="ws-loader-gif"><i class="fa fa-circle-o-notch fa-spin fa-3x fa-fw"></i></div>
    </div>

    <!--New header section -->
    <header class="site_header">
        <div class="header_container">
            <div class="site_logo_block">
                <a href="/"><img src="images/logo.png" alt="" title="" /></a>
            </div>

            <div class="header_right_block">

                <div class="site-nav-collapse">
                    <nav class="cssCustomMenu">
                        <label for="drop" class="css_toggle">&#8801;</label>
                        <input type="checkbox" id="drop" />
                        <ul class="cssMenu">
                            <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                            <li><a asp-controller="Service" asp-action="Index">View Girls</a></li>
                            <li><a asp-controller="Service" asp-action="IndexTown">View Cities</a></li>
                            <li><a asp-controller="Service" asp-action="IndexMassage">View Massages</a></li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </header>


    <!--end New header section -->
    @* ///////////////////////////// *@

    <div id="page" class="hfeed">

        <div id="wsBody">
            <div id="wsHome">
                <div id="main">

                    @RenderBody()
                    
                    <hr class="ws-clear">

                </div>

                <hr class="ws-clear">

            </div>

            <hr class="ws-clear">

        </div>
    </div>

    @* ///////////////////////////// *@

    <footer id="wsFoot">
        <nav class="ws-footer-nav">
            <div class="ws-wrap">
                <div class="menu-footer-menu-container">
                    <ul id="menu-footer-menu" class="menu">
                        <li id="menu-item-908" class="menu-item"><a asp-action="Index">Home</a></li>
                        <!--<li id="menu-item-907" class="menu-item"><a href="#">Cities</a></li>-->
                        <li id="menu-item-904" class="menu-item"><a asp-action="Booking">Booking</a></li>
                        <li id="menu-item-903" class="menu-item"><a asp-action="Contact">Contact</a></li>
                    </ul>
                </div>
            </div>
        </nav>

        <div class="ws-wrap">
            <div class="divleft">
                <div id="wsCredits">©2018-@DateTime.Now.Year Massage Girls Online. All Rights Reserved.</div>
                <div id="wsInfo"><i class="fa fa-mobile"></i> 463-209-7850</div>
            </div>

            <hr class="ws-clear">
            <div onclick="scrollToTop()" id="wsBackTop" class="ws-button"><i class="fa fa-angle-up fa-2x" aria-hidden="true"></i></div>

        </div>
        <partial name="_LoginPartial" />

    </footer>

    <script>
        function scrollToTop() {
            window.scrollTo({ top: 0, behavior: 'smooth' });
        }
    </script>

    <script type="text/javascript" async
            src="https://chat.serverpipe.com:8443/invite.js?domain=@Constants.SiteName"></script>

    <div name="woInvite" id="woInvite" align="right" style="position: fixed; left: 0px; z-index: 999;"></div>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>